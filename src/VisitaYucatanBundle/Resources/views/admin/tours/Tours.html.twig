{% extends '::BaseTemplate.html.twig' %}

{% block breadcrumb %}

    {{ parent() }}

{% endblock %}


{% block content %}
    <div class="content-main" data-ng-app="Currency" data-ng-controller="CurrencyController as ctrlCurrency"
         ng-init="ctrlCurrency.init('{% trans %}catalog.currency.report.message.new.currency{% endtrans %}', '{% trans %}catalog.currency.report.message.edit.currency{% endtrans %}',
         '{% trans %}catalog.currency.report.message.success.create{% endtrans %}', '{% trans %}catalog.currency.report.message.success.update{% endtrans %}',
         '{% trans %}catalog.currency.report.message.delete{% endtrans %}','{% trans %}catalog.currency.report.message.error{% endtrans %}', '{% trans %}general.message.loading{% endtrans %}')">
        <div class="row">
            <div class="col-md-12">

                <div class="panel panel-bluegray" data-widget="{&quot;draggable&quot;: &quot;false&quot;}" data-widget-static="" style="visibility: visible; opacity: 1; display: block; transform: translateY(0px);">
                    <div class="panel-heading">
                        <h2>
                            {% trans %}catalog.currency.report.label.title{% endtrans %}
                        </h2>
                    </div>
                    <div class="panel-body">

                        <div data-widget-group="group1">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h2>Data Tables</h2>
                                            <div class="panel-ctrls"></div>
                                        </div>
                                        <div class="panel-body no-padding">
                                            <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                                                <thead>
                                                <tr>
                                                    <th>Rendering engine</th>
                                                    <th>Browser</th>
                                                    <th>Platform(s)</th>
                                                    <th>Engine version</th>
                                                    <th>CSS grade</th>
                                                </tr>
                                                </thead>
                                                <tbody>
                                                <tr class="odd gradeX">
                                                    <td>Trident</td>
                                                    <td>Internet
                                                        Explorer 4.0</td>
                                                    <td>Win 95+</td>
                                                    <td class="center"> 4</td>
                                                    <td class="center">X</td>
                                                </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="panel-footer"></div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>

        {#{% include '@VisitaYucatan/admin/catalogs/currency/ModalCurrency.html.twig' %}#}

        <!--Paths-->
        <input type="hidden" id="pathListCurrency" value="{{ path('moneda_find_list') }}"/>
        <input type="hidden" id="pathCreateCurrency" value="{{ path('moneda_crear') }}"/>
    </div>

{% endblock %}

{% block scripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/VisitaYucatanBundle/js/catalogs/currency/CurrencyProvider.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/VisitaYucatanBundle/js/catalogs/currency/CurrencyController.js') }}" type="text/javascript"></script>
{% endblock %}